{
  "openapi": "3.0.1",
  "info": {
    "title": "WeatherForecast Plugin",
    "description": "Call the support microsoft for get the WeatherForecast",
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://localhost:7061"
    }
  ],
  "paths": {
    "/WeatherForecast": {
      "get": {
        "operationId": "GetWeatherForecast",
        "summary": "Get the list of WeatherForecast",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WeatherForecast"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "WeatherForecast": {
        "type": "array",
        "item": {
          "date": "string",
          "temperatureC": "int32",
          "temperatureF": "int32",
          "summary": "string"
        },
        "description": "The list of WeatherForecast."
      },
    }
  }
}
